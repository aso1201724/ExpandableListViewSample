    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.expandablelistviewsample.ExpandableListViewSample"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.expandablelistviewsample.MsgActivity"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name="com.example.expandablelistviewsample.Test1Activity"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name="com.example.expandablelistviewsample.Test2Activity"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name="com.example.expandablelistviewsample.TeacherActivity"
            android:label="@string/app_name" >
        </activity>

    </application>

</manifest>
